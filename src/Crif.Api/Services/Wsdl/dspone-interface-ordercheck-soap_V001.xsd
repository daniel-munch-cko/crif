<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ordercheck="http://www.deltavista.com/dspone/ordercheck-if/V001" targetNamespace="http://www.deltavista.com/dspone/ordercheck-if/V001" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.60">
	<xs:element name="orderCheckRequest" type="ordercheck:OrderCheckRequest">
		<xs:annotation>
			<xs:documentation>OrderCheck request root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="orderCheckResponse" type="ordercheck:OrderCheckResponse">
		<xs:annotation>
			<xs:documentation>OrderCheck response root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="messageContext" type="ordercheck:MessageContext">
		<xs:annotation>
			<xs:documentation>MessageContext root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="MessageContext">
		<xs:annotation>
			<xs:documentation>Header structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="credentials" type="ordercheck:Credentials">
				<xs:annotation>
					<xs:documentation>data for authentication and authorization</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="correlationID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>unique ID to link request to response</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Credentials">
		<xs:annotation>
			<xs:documentation>Header - credentials definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="user" type="xs:string">
				<xs:annotation>
					<xs:documentation>user name as in system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="password" type="xs:string">
				<xs:annotation>
					<xs:documentation>user password as in system</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderCheckRequest">
		<xs:annotation>
			<xs:documentation>OrderCheckRequest structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="product" type="ordercheck:Product">
				<xs:annotation>
					<xs:documentation>parameters of requested product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="archiveID" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>archive ID from Archive instead of searchedAddress</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="searchedAddress" type="ordercheck:SearchedAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>search criteria for person or company - subject of the creditworthiness assessment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="additionalAddress" type="ordercheck:AdditionalAddress" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>additional addresses to be used in processing</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="clientData" type="ordercheck:ClientData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>client-specific data </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Product">
		<xs:annotation>
			<xs:documentation>Basic Product definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string">
				<xs:annotation>
					<xs:documentation>product identifier</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>product version</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="ordercheck:Country" minOccurs="0">
				<xs:annotation>
					<xs:documentation>points to the local system for product call</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="language" type="ordercheck:Language" minOccurs="0">
				<xs:annotation>
					<xs:documentation>language for translatable values in response</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="proofOfInterest" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>proof of interest for the request</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalAddress">
		<xs:annotation>
			<xs:documentation>Client may send additional addresses to be used in processing</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ordercheck:SearchedAddress">
				<xs:sequence>
					<xs:element name="type" type="xs:string">
						<xs:annotation>
							<xs:documentation>classification of a multiple entries (former, bill, delivery, employer, related company, owner, etc.)</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SearchedAddress">
		<xs:annotation>
			<xs:documentation>SearchCandidate structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="deltavistaID" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Deltavista ID of a company/person address</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="registry" type="ordercheck:CompanyRegistryID" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>company can be identified by its registration number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="legalForm" type="ordercheck:LegalForm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>legal form: private or company</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="address" type="ordercheck:Address" minOccurs="0">
				<xs:annotation>
					<xs:documentation>data of person or company to check</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="ordercheck:Contact" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>contact data of the client</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customer" type="ordercheck:CustomerData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>identification data of the customers as in the client system</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CompanyRegistryID">
		<xs:annotation>
			<xs:documentation>CompanyRegistryID structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type" type="xs:string">
				<xs:annotation>
					<xs:documentation>type of registration (commercial or trade registry)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="number" type="xs:string">
				<xs:annotation>
					<xs:documentation>registry number of the company</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zip" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>zip of registry office (e.g. court)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="city" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>city of registry office (e.g. court)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Address">
		<xs:annotation>
			<xs:documentation>Address data definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>company name or last name of private person</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>first name of private person (empty for company, except PPC)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="maidenName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>maiden name of  person (empty for company, except PPC)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gender" type="ordercheck:Gender" minOccurs="0">
				<xs:annotation>
					<xs:documentation>sex of private person (empty for company, except PPC)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dateOfBirth" type="ordercheck:Date_YYYYMMDD" minOccurs="0">
				<xs:annotation>
					<xs:documentation>date of birth of private person (empty for company, except PPC)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="location" type="ordercheck:Location" minOccurs="0">
				<xs:annotation>
					<xs:documentation>location data</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="LocationWithType">
		<xs:sequence>
			<xs:element name="type" type="xs:string">
				<xs:annotation>
					<xs:documentation>Location data definition</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="street" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>name of the street</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="house" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>house number (with eventual suffixes)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="houseExtension" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>extension number, e.g. flat number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="city" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>name of the city</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zip" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>zip code of location</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="ordercheck:Country" minOccurs="0">
				<xs:annotation>
					<xs:documentation>country of the person or company</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Location">
		<xs:annotation>
			<xs:documentation>Location data definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="street" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>name of the street</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="house" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>house number (with eventual suffixes)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="houseExtension" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>extension number, e.g. flat number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="city" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>name of the city</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="zip" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>zip code of location</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="country" type="ordercheck:Country" minOccurs="0">
				<xs:annotation>
					<xs:documentation>country of the person or company</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderCheckResponse">
		<xs:annotation>
			<xs:documentation>OrderCheckResponse structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="returnCode" type="ordercheck:ReturnCode">
				<xs:annotation>
					<xs:documentation>return code, available values: 
	0 - nothing found, 
	1 - report, 
	2 - candidate list  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="product" type="ordercheck:Product" minOccurs="0">
				<xs:annotation>
					<xs:documentation>description of returned product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="archiveID" type="xs:long">
				<xs:annotation>
					<xs:documentation>unique ID identifying report in Archive
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reportCreationTime" type="ordercheck:Date_YYYYMMDDHHmm">
				<xs:annotation>
					<xs:documentation>time of report generation (stored in archive)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="foundAddress" type="ordercheck:FoundAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>data of found person or company ("Beauskunftet")</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="myDecision" type="ordercheck:MyDecision" minOccurs="0">
				<xs:annotation>
					<xs:documentation>decision section of the report</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="personBasicData" type="ordercheck:PersonBasicData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>basic data of person</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="companyBasicData" type="ordercheck:CompanyBasicData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>basic data of company</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="decisionMatrix" type="ordercheck:DecisionMatrix" minOccurs="0">
				<xs:annotation>
					<xs:documentation>the decision matrix elements</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="paymentHistory" type="ordercheck:PaymentHistory" minOccurs="0">
				<xs:annotation>
					<xs:documentation>information about negative payment behaviors</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="similarHitAddress" type="ordercheck:SimilarCandidate" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>list of similar addresses or candidate list</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="normalization" type="ordercheck:Normalization" minOccurs="0">
				<xs:annotation>
					<xs:documentation>part of IdentificationProcess Section</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="candidate" type="ordercheck:SimilarCandidate" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>list of candidates returned instead of product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="clientExtensions" type="ordercheck:ClientExtensions" minOccurs="0">
				<xs:annotation>
					<xs:documentation>client specific extensions of the product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="executionStrategy" type="ordercheck:ExecutionStrategy" minOccurs="0">
				<xs:annotation>
					<xs:documentation>details of selected check strategy (RCO)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IdentifiableAddress">
		<xs:annotation>
			<xs:documentation>IdentifiableAddress structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="legalForm" type="ordercheck:LegalForm">
				<xs:annotation>
					<xs:documentation>legal form: private or company</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="address" type="ordercheck:Address">
				<xs:annotation>
					<xs:documentation>data of a person or a company</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="addressID" type="xs:long" minOccurs="0">
				<xs:annotation>
					<xs:documentation>unique ID for address in Deltavista Registry</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ClientData">
		<xs:annotation>
			<xs:documentation>ClientSpecific data structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="userReference" type="ordercheck:UserReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>reference data of the user at client site</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="reference" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>client's internal reference for customer (contract, offer,..)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankAccount" type="ordercheck:BankAccountDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>bank account details or credit card details</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="order" type="ordercheck:Order" minOccurs="0">
				<xs:annotation>
					<xs:documentation>data about the order, application (web shop basket, credit, etc)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--
			<xs:element name="customer" type="ordercheck:CustomerData" minOccurs="0">
				<xs:annotation>
					<xs:documentation>identification data of the customers as in the client system</xs:documentation>
				</xs:annotation>
			</xs:element>
			-->
			<xs:element name="additionalData" type="ordercheck:AdditionalDataItem" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>generic collection of client data extending the standard OC</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="UserReference">
		<xs:annotation>
			<xs:documentation>UserReference data structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="userID" type="xs:string">
				<xs:annotation>
					<xs:documentation>client's internal user id</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="name" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>last name of the user</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>first name of the client's user</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orgUnit" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>organization unit the client's user belongs to</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="costCenter" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>client's cost center</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalDataItem">
		<xs:annotation>
			<xs:documentation>AdditionalData item structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="xs:string">
				<xs:annotation>
					<xs:documentation>name of the client's additional data </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="value" type="xs:string">
				<xs:annotation>
					<xs:documentation>client element value</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Identification">
		<xs:annotation>
			<xs:documentation>Response section - Identification - root structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="foundCandidate" type="ordercheck:FoundAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>data of found person or company ("Beauskunftet")</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FoundAddress">
		<xs:annotation>
			<xs:documentation>FoundAddress structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="legalForm" type="ordercheck:LegalForm">
				<xs:annotation>
					<xs:documentation>legal form: private or company</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="address" type="ordercheck:Address">
				<xs:annotation>
					<xs:documentation>data of found person or company</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MyDecision">
		<xs:annotation>
			<xs:documentation>Response section - MyDecision - root structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="decision" type="ordercheck:TrafficLight">
				<xs:annotation>
					<xs:documentation>traffic light (green, yellow, red)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="recommendation" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>textual hint for handling</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="limit" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>credit limit of Deltavista or of the client</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="ordercheck:Currency" minOccurs="0">
				<xs:annotation>
					<xs:documentation>currency of the credit limit</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PersonBasicData">
		<xs:annotation>
			<xs:documentation>Response section - PersonBasicData - root structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="knownSince" type="ordercheck:Date_YYYYMMDD" minOccurs="0">
				<xs:annotation>
					<xs:documentation>entry date of first address of the person to the system</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="contact" type="ordercheck:Contact" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>contact data</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="environment" type="ordercheck:Environment" minOccurs="0">
				<xs:annotation>
					<xs:documentation>environment data for persons only</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Environment">
		<xs:annotation>
			<xs:documentation>Environment data definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="householdSize" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>size classification of a household</xs:documentation>
