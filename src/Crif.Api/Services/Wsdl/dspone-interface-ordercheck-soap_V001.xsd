	<xs:complexType name="Order">
		<xs:annotation>
			<xs:documentation>Order structure definition</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="orderTimestamp" type="ordercheck:Date_YYYYMMDDHHmm" minOccurs="0">
				<xs:annotation>
					<xs:documentation>date and time of order placement</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>identifier of the placed order</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="currency" type="ordercheck:Currency" minOccurs="0">
				<xs:annotation>
					<xs:documentation>currency of the order items and values</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderItem" type="ordercheck:OrderItem" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>list of goods/services ordered by the customer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="orderValue" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>value of order items (it can be different than sum of order item values sent in the request, e.g. due to limitation of orderItem number)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="totalValue" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>total value of order (it may include additional charges or discounts, shipment cost, etc.)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actualBalance" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>total value of not yet paid orders (regardless of if exported to Customer List or not)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="balanceAdjustment" type="xs:float" minOccurs="0">
				<xs:annotation>
					<xs:documentation>value of new not yet paid orders since  the last Customer List update</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--
			<xs:element name="contact" type="ordercheck:Contact" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>contact data of the client</xs:documentation>
				</xs:annotation>
			</xs:element>
			-->
			<xs:element name="riskClass" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>classification of order with respect to risk of default</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="channel" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>sales channel (web, phone, letter,…), point of sales</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
